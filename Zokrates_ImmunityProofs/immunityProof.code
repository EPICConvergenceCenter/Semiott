def main(private field suspected, private field recovered, private field antibodyTest) ->(field):
    field category1 = if suspected == 1 then 0 else 1 fi
    field category2 = if recovered == 1 then 1 else 0 fi
    field category3 = if antibodyTest == 1 then 0 else 1 fi
    return category1*category2*category3  