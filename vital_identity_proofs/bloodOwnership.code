    field b = if DonorAmount == 25000 then 1 else 0 fi
    field c = a*b
    field o1 = if BloodType == 11143 then 1 else 0 fi
    field o0 = if BloodType == 11145 then 1 else 0 fi
    field a1 = if BloodType == 9743 then 1 else 0 fi
    field a0 = if BloodType == 9745 then 1 else 0 fi
    field b1 = if BloodType == 9843 then 1 else 0 fi
    field b0 = if BloodType == 9845 then 1 else 0 fi
    field ab1 = if BloodType == 989743 then 1 else 0 fi
    field ab0 = if BloodType == 989745 then 1 else 0 fi
    field blood = o1 + o0 + a1 + a0 + b1 + b0 + ab1 + ab0
